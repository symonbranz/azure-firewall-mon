{
  "routes": [
    {
      "route": "/.auth/login/github",
      "statusCode": 403
    },
    {
      "route": "/*",
      "allowedRoles": [
        "authenticated"
      ]
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/aad?post_login_redirect_uri=.referrer",
      "statusCode": 302
    },
    "404": {
      "redirect": "/",
      "statusCode": 302
    }
  }
}
{
  "networking": {
    "allowedIpRanges": [
      "10.0.0.0/24",
      "100.0.0.0/32",
      "192.168.100.0/22",
      "163.7.129.178/32"
    ]
  }
}
